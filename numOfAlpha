INCLUDE Irvine32.inc

.DATA

arr byte "24K0909", 0  ; ROLL NO: 24K-0909
arr2 byte "Number of alphabets: ", 0
.CODE
 MAIN PROC

 MOV EDX, OFFSET arr
 call WriteString
 call Crlf

 MOV ESI, OFFSET ARR
 MOV ECX, 0

 countLoop:

 MOV AL, [ESI]
 CMP AL, 0
 JE doneCount

 CMP AL, 'A'
 JB notAlpha

 cmp al, 'Z'
 JBE isAlpha

 CMP AL, 'a'
 JB notAlpha

 cmp al, 'z'
 JBE isAlpha
 
 notAlpha:
 inc esi
 jmp countLoop

 isAlpha:
 inc ecx
 inc esi
 jmp countLoop

 doneCount:
 mov EDX, OFFSET arr2
 call WriteString
 mov eax, ecx
 call WriteInt

 exit

 main ENDP
 END main


 
